<%- if controller_name != 'sessions' %>
  <%= link_to "Sign in", new_session_path(resource_name), class: "btn btn-primary shadow fs-5" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-primary shadow fs-5" %><br />
<% end %>